{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "src/NewBackend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/NewBackend/ContactForm/SendEmail.mjs",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/NewBackend/server.js"
    },
    {
      "source": "/send-enquiry",
      "destination": "/src/NewBackend/ContactForm/SendEmail.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "text/html"
        }
      ]
    }
  ]
}
