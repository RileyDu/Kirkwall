{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "src/NewBackend/api.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "src/NewBackend/ContactForm/SendEmail.mjs",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/NewBackend/api.mjs"
    },
    {
      "source": "/send-enquiry",
      "destination": "/src/NewBackend/ContactForm/SendEmail.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/"

    }
  ]
}
