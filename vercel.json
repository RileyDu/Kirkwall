{
  "version":2,
  "builds":[
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
      "distDir": "build"
      }
      },
     {
        "src":"src/NewBackend/index.js",
        "use":"@vercel/node"
     }
  ],
  "rewrites":[
     {
        "source":"/api/(.*)",
        "destination":"src/NewBackend"
     }
  ]
}