{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "src/Backend/ContactForm/SendEmail.mjs",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/send-enquiry",
      "destination": "src/Backend/ContactForm/SendEmail.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
