{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/send-enquiry",
      "destination": "/api/ContactForm/SendEmail" 
    },
    {
      "source": "/(.*)",
      "destination": "/" 
    }
  ]
}
